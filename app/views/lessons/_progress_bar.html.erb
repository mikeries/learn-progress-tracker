
<% progress = element.completed_lessons.to_f / element.lessons.count %>
<% style = "width:#{progress*100}%" %>

<div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow='<%= "#{progress}" %>'
        aria-valuemin="0" aria-valuemax="100" style='<%=style%>'>
  </div>
</div>
